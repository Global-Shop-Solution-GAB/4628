Program.Sub.loadDTAll.Start
	F.Intrinsic.Control.Try
	v.Local..BulkDeclareString(sError, sSQL)
	
	Gui.FrmOrderReview.lblStatus.Visible(True)

	F.Intrinsic.Control.If(V.DataTable.dtAll.Exists,=,True)
		F.Data.DataTable.Close("dtAll")
	F.Intrinsic.Control.EndIf
	V.Local.sSQL.Set("Select Distinct VOBT.NAME_CUSTOMER,CODE_ZIP_SHIP,VOL.PART,IM.CODE_SORT,IF(VOH.CUSTOMER_PO <> '',VOH.CUSTOMER_PO,VJH.CUSTOMER_PO) AS CUSTOMER_PO,VOL.ORDER_NO,LEFT(VOL.RECORD_NO,3) AS RECORD_NO,CASE WHEN VOL.FLAG_USE_MQD != 'Y' THEN VOL.QTY_BO WHEN FLAG_USE_MQD = 'Y' THEN VOSS.QTY_OPEN END AS QTY_BO,IM.QTY_ONHAND,CASE WHEN VOL.FLAG_USE_MQD != 'Y' THEN VOL.DATE_ITEM_PROM WHEN FLAG_USE_MQD = 'Y' THEN VOSS.CUST_DUE_DATE END AS DATE_ITEM_PROM,CASE WHEN VJH.DATE_DUE = '1900-1-1' THEN NULL ELSE VJH.DATE_DUE END AS DATE_JOB_DUE,VJH.JOB,VJH.SUFFIX,VOH.SALESPERSON,VOH.TYPE_COMMISSION,VOL.LOCATION,VOL.DESCRIPTION,VOL.PRODUCT_LINE,CASE WHEN VOTW.DATE_ORDER_DUE = '1900-1-1' THEN NULL ELSE VOTW.DATE_ORDER_DUE END AS DATE_ORDER_DUE,CASE WHEN VOL.FLAG_USE_MQD = 'Y' THEN CONVERT(1,SQL_BIT) ELSE CONVERT(0,SQL_BIT) END AS FLAG_USE_MQD,VJH.QTY_ORDER,VJH.QTY_ORDER AS WO_QTY,NOTES,VOL.USER_4,VOL.USER_5,COALESCE(VJH.JOB+VJH.SUFFIX,'') As JS FROM V_ORDER_LINES VOL LEFT JOIN V_ORDER_TO_WO VOTW ON VOL.ORDER_NO = VOTW.ORDER_NO AND VOL.RECORD_NO = VOTW.ORDER_LINE LEFT JOIN V_JOB_HEADER VJH ON VOTW.JOB = VJH.JOB AND VOTW.SUFFIX = VJH.SUFFIX LEFT JOIN V_OE_SHIP_SCHED VOSS ON VOSS.ORDER_NO = VOL.ORDER_NO AND VOSS.ORDER_LINE = VOL.RECORD_NO LEFT JOIN V_ORDER_HEADER VOH ON VOL.ORDER_NO = VOH.ORDER_NO LEFT JOIN V_ORDER_BILL_TO VOBT ON VOL.ORDER_NO = VOBT.ORDER_NO LEFT JOIN GAB_4167_SO_NOTES A ON VOL.ORDER_NO = A.SO AND VOL.RECORD_NO = CONCAT(A.SOLINE,0) JOIN V_ORDER_SHIP_TO VOST ON VOH.ORDER_NO = VOST.ORDER_NO AND VOH.SHIP_ID = VOST.ORDER_SHIP_ID JOIN V_INVENTORY_MSTR IM ON VOL.PART = IM.PART AND VOL.LOCATION = IM.LOCATION WHERE QTY_BO > 0 ORDER BY DATE_ITEM_PROM;")
	F.Data.DataTable.CreateFromSQL("dtAll","con",V.Local.sSQL,True)
	
	F.Data.DataTable.AddColumn("dtAll","WO_Clsd",Boolean,False)
	F.Data.Dictionary.CreateFromSQL("dWOClsd","con","Select Job+Suffix As JS,Cast(1 As Bit) From V_Job_Header Where Date_Closed <> '1900-01-01';")
	F.Data.Dictionary.SetDefaultReturn("dWOClsd",False)
	F.Data.DataTable.FillFromDictionary("dtAll","dWOClsd","JS","WO_Clsd")
	F.Data.Dictionary.Close("dWOClsd")

	F.Intrinsic.Control.CallSub(loaddvgvAll)
	
	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: GCG_4628_OE_OpenSOReviewTiedWOs.g2u{0}{0}Subroutine: {1}{0}Error Occurred {2} with description {3}.",V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.loadDTAll.End

